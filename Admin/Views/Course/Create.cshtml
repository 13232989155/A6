                                                <label>
                                                    &nbsp;
                                                </label>
                                            </h3>
                                            <h3 class="name">
                                                <label for="fileCoverVideo" style="cursor:pointer">
                                                    <i class="fa fa-plus fa-5x"></i>
                                                </label>
                                            </h3>
