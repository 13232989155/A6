        $.when(GetQiNiuToken(bucket)).done(function (defer) {
            var reader = new FileReader();
            reader.readAsDataURL(file);
            reader.onload = function (e) {
                blob = DataURItoBlob(e.target.result);
                var token
                var key
                var blob

                token = defer

                var guid = new GUID();
                key = guid.newGUID();

                var putExtra = {
                    fname: "",  //文件原文件名
                    params: {}, //用来放置自定义变量
                    mimeType: null  //用来限制上传文件类型，为 null 时表示不对文件类型限制；限制类型放到数组里： ["image/png", "image/jpeg", "image/gif"]
                };

                var config = {
                    useCdnDomain: true,   //表示是否使用 cdn 加速域名，为布尔值，true 表示使用，默认为 false。
                    region: qiniu.region.z2     // 根据具体提示修改上传地区,当为 null 或 undefined 时，自动分析上传域名区域
                };

                var observable = qiniu.upload(blob, key + suffix, token, putExtra, config)
                new PNotify({
                    title: '封面图片文件上传中!',
                    text: '请等待...',
                    type: 'info',
                    delay: 1000,
                    styling: 'bootstrap3',
                    addclass: 'dark'
                });
                var subscription = observable.subscribe({
                    next: (res) => {
                    },
                    error: (err) => {
                        // 失败报错信息
                        ShowPNotify(err);
                    },
                    complete: (res) => {
                        // 接收成功后返回的信息
                        ShowSuccessPNotify("封面图片文件上传完成")
                        courseEntity.coverImage = GetQiNiuDomain(bucket) + res.key
                    }
                })
            }
        });
    }

    var courseEntity = {
        courseTypeId: "",
        name: "",
        describe: "",
        coverImage: "",
        coverVideo: "",
        teacherDescribe: "",
        videoUrl: "",
        tips: "",
        price: ""
    }

    var courseVue = new Vue({
        el: "#example",
        data: courseEntity,
        methods: {
            DeleteVideo: function () {
                courseEntity.videoUrl = "";
            },
            UpdateVideo: function () {
                $("#fileVideo").trigger("click");
            },
            DeleteCoverVideo: function () {
                courseEntity.coverVideo = "";
            },
            UpdateCoverVideo: function () {
                $("#fileCoverVideo").trigger("click");
            },
            DeleteCoverImage: function () {
                courseEntity.coverImage = "";
            },
            UpdateCoverImage: function () {
                $("#fileCoverImage").trigger("click");
            }
        }
    })

    function Save() {

        if (courseEntity.name == "") {
            ShowPNotify("名称不能为空");
            return
        }

        if (courseEntity.price == "") {
            ShowPNotify("价格不能为空");
            return
        }

        if (courseEntity.videoUrl == "") {
            ShowPNotify("课程视频不能为空");
            return
        }

        if (courseEntity.coverImage == "") {
            ShowPNotify("封面图片不能为空");
            return
        }

        if (describeEditor.txt.html() == "") {
            ShowPNotify("课程介绍不能为空");
            return
        }
        courseEntity.describe = describeEditor.txt.html();

        if (teacherDescribeEditor.txt.html() == "") {
            ShowPNotify("教师介绍不能为空");
            return
        }
        courseEntity.teacherDescribe = teacherDescribeEditor.txt.html();
        courseEntity.tips = tipsEditor.txt.html();
        $.ajax({
            type: "post",
            url: "/Course/Create",
            data: {
                courseTypeId: courseEntity.courseTypeId,
                name: courseEntity.name,
                describe: courseEntity.describe,
                coverImage: courseEntity.coverImage,
                coverVideo: courseEntity.coverVideo,
                teacherDescribe: courseEntity.teacherDescribe,
                videoUrl: courseEntity.videoUrl,
                tips: courseEntity.tips,
                price: courseEntity.price
            },
            dataType: "json",
            success: function (result) {
                if (result.code == "200") {
                    window.location.href = "/Course/List";
                }
                else {
                    ShowPNotify(result.msg)
                }
            },
            error: function () {
                ShowPNotify("请求错误")
            },
            complete: function (res) {
                $("body").mLoading("hide");
            }
        });

    }

